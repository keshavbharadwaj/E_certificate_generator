<!DOCTYPE html>
<html>
  <head>
    <title>Verified</title>
    <style>
        #heading{background-color: rgb(8, 209, 58); width: 100%; height: 200px;}
        h1{position: relative; left: 40%; top: 30%;  color: white; font-size: 70px;}
        #message{position: relative; left: 33%; font-size: 50px;}
        p{font-size: 15px;}
        #box{position: relative; left: 43%; border: solid black 5px; width: fit-content; padding: 5px;}
    </style>
  </head>
  <body>
    <div id="heading">
        <h1>Verified!</h1>
    </div>   
    <p id="message">This certificate is authentic</p>
    <div id="box">

      <ul>
        <% for(var d in data) { %>
        <% if (data[d] === '') { %>
        <% continue; } %>   
        <li>
          <p><%= d %> : <%= data[d] %></p>
        </li>
      <% } %> 
      </ul>
    </div>
  </body>
</html>
